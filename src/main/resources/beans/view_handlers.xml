<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dota2HeroGridCustomizer-addHeroTranslationsByFileHandler"
          class="drakonli.dota2.hero_grid_customizer.view.load.handler.AddHeroTranslationsByFileHandler">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroNamesByFileImporter"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroTranslationViewModelsToEntityMapper"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroTranslationsToViewModelMapper"/>
    </bean>

    <bean id="dota2HeroGridCustomizer-restoreHeroNamesHandler"
          class="drakonli.dota2.hero_grid_customizer.view.restore.handler.RestoreHeroNamesHandler">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroNamesByFileStorageRestorer"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroTranslationViewModelsToEntityMapper"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroTranslationsToViewModelMapper"/>
    </bean>
</beans>