<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="drakonli.dota2.hero_grid_customizer.view.hero_translations_table.HeroTranslationsTableView"
          class="drakonli.dota2.hero_grid_customizer.view.hero_translations_table.HeroTranslationsTableView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.view.load.LoadHeroNamesFileButtonView"
          class="drakonli.dota2.hero_grid_customizer.view.load.LoadHeroNamesFileButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-fileChooserFactory-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg>
            <list>
                <ref bean="dota2HeroGridCustomizer-addHeroTranslationsByFileHandler"/>
            </list>
        </constructor-arg>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.view.save.SaveHeroNamesIntoFileButtonView"
          class="drakonli.dota2.hero_grid_customizer.view.save.SaveHeroNamesIntoFileButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg>
            <list>
                <ref bean="dota2HeroGridCustomizer-backupHeroTranslationFileHandler"/>
                <ref bean="dota2HeroGridCustomizer-storeHeroNamesHandler"/>
                <ref bean="dota2HeroGridCustomizer-replaceHeroNamesInTranslationsFileHandler"/>
            </list>
        </constructor-arg>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.view.restore.RestoreHeroNamesButtonView"
          class="drakonli.dota2.hero_grid_customizer.view.restore.RestoreHeroNamesButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg>
            <list>
                <ref bean="dota2HeroGridCustomizer-restoreHeroNamesHandler"/>
            </list>
        </constructor-arg>
    </bean>

</beans>