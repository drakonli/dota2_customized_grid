<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="drakonli.dota2.hero_grid_customizer.ui.hero_translations_table.HeroTranslationsTableView"
          class="drakonli.dota2.hero_grid_customizer.ui.hero_translations_table.HeroTranslationsTableView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.load.LoadHeroNamesFileButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.load.LoadHeroNamesFileButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-exportImportByFileVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-fileChooserFactory-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-loadHeroNamesFileButtonViewHandlers"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.save.SaveHeroNamesIntoFileButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.save.SaveHeroNamesIntoFileButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-saveHeroNamesIntoFileButtonViewHandlers"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.restore.RestoreHeroNamesButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.restore.RestoreHeroNamesButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-restoreHeroNamesButtonViewHandlers"/>
    </bean>

</beans>