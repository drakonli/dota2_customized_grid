<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.table.GridCustomizationTableView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.table.GridCustomizationTableView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.from_file.ImportGridCustomizationFromFileMenuItemView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.from_file.ImportGridCustomizationFromFileMenuItemView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-fileChooserFactory-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-importHeroGridCustomizationFromFileAction"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.export_customization.into_current_file.ExportGridCustomizationIntoCurrentFileButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.export_customization.into_current_file.ExportGridCustomizationIntoCurrentFileButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-exportHeroGridCustomizationIntoFileAction"/>
        <constructor-arg ref="dota2HeroGridCustomizer-exportImportByFileVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.by_latest_export.ImportGridCustomizationByLatestExportMenuItemView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.by_latest_export.ImportGridCustomizationByLatestExportMenuItemView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
        <constructor-arg ref="dota2HeroGridCustomizer-exportImportHeroGridByLatestSaveVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-importHeroGridCustomizationFromStorageByLatestSaveAction"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.clear_button.ClearGridCustomizationButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.clear_button.ClearGridCustomizationButtonView"
          scope="prototype">
        <constructor-arg ref="dota2HeroGridCustomizer-heroGridVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-exportImportByFileVM"/>
        <constructor-arg ref="dota2HeroGridCustomizer-notificator-main"/>
    </bean>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.ImportGridCustomizationMenuView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.import_customization.ImportGridCustomizationMenuView"
          scope="prototype"/>

    <bean id="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.export_customization.ExportGridCustomizationMenuButtonView"
          class="drakonli.dota2.hero_grid_customizer.ui.view.menu_bar.export_customization.ExportGridCustomizationMenuButtonView"
          scope="prototype"/>
</beans>